{
  "name": "sein-helper-js",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "proto": "npm run proto-code && npm run proto-types ",
    "proto-code": "protoc -I ../../go/desktop-helper/protodef --js_out=import_style=commonjs,binary:./lib/pb --grpc_out=./lib/pb --plugin=protoc-gen-grpc=./node_modules/grpc-tools/bin/grpc_node_plugin {auth,utils}.proto",
    "proto-types": "protoc -I ../../go/desktop-helper/protodef --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=./@types {auth,utils}.proto"
  },
  "dependencies": {
    "get-port": "^4.0.0",
    "grpc": "^1.16.1",
    "sein-helper": "file:../../go/desktop-helper"
  },
  "devDependencies": {
    "@types/get-port": "^4.0.0",
    "@types/node": "^10.12.12",
    "google-protobuf": "^3.6.1",
    "grpc-tools": "^1.6.6",
    "grpc_tools_node_protoc_ts": "^2.4.1",
    "sein-tslint": "file:../shared/tslint",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Seinapp/sein.git"
  },
  "author": "Melvin Laplanche",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Seinapp/sein/issues"
  },
  "homepage": "https://github.com/Seinapp/sein"
}
